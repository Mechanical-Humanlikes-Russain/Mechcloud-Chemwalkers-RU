<?xml version="1.0" encoding="utf-8" ?>
<Defs>

    <!-- This Rulepackdef is what finds the txt files to be used in the later rulepacks -->
    <RulePackDef>
        <defName>ABF_ChemwalkerNameRules</defName>
        <rulePack>
            <rulesFiles>
                <li>Mechcloud->ABF_ChemwalkerNames/MechcloudNames</li>
                <li>Lumbering->ABF_ChemwalkerNames/LumberingNames</li>
                <li>Chemwalker->ABF_ChemwalkerNames/ChemwalkerNames</li>
            </rulesFiles>
        </rulePack>
    </RulePackDef>

    <!-- This is the item that goes into the racedef's namegenerator
        Example - <nameGenerator>ATR_AndroidMaleNames</nameGenerator> -->
    <RulePackDef>
        <defName>ABF_MechcloudNames</defName>
        <include>
            <li>ABF_ChemwalkerNameRules</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=12)->[Mechcloud] [Chemwalker]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

    <RulePackDef>
        <defName>ABF_LumberingNames</defName>
        <include>
            <li>ABF_ChemwalkerNameRules</li>
        </include>
        <rulePack>
            <rulesStrings>
                <li>r_name(p=12)->[Lumbering] [Chemwalker]</li>
            </rulesStrings>
        </rulePack>
    </RulePackDef>

</Defs>